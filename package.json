{
  "name": "deck.gl-web-demo",
  "version": "1.0.0",
  "description": "Demo page for deck.gl",
  "main": "index.js",
  "dependencies": {
    "babel-plugin-glslify": "^1.0.2",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-polyfill": "^6.13.0",
    "babel-runtime": "^6.11.6",
    "d3-color": "^1.0.1",
    "d3-request": "^1.0.2",
    "d3-scale": "^1.0.3",
    "deck.gl": "3.0.0-beta17",
    "glslify": "^5.1.0",
    "highlight.js": "^9.7.0",
    "marked": "^0.3.6",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-map-gl": "^1.6.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-plugin-glslify": "^1.0.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "babelify": "^7.2.0",
    "browserify": "13.0.0",
    "budo": "^8.0.3",
    "sassify": "^2.0.0",
    "uglify-js": "^2.6.1"
  },
  "scripts": {
    "start": "budo src/javascripts/main.js:main.min.js --dir src/ --open --  -t sassify -t babelify -t glslify",
    "build": "npm run build-clean && npm run build-copy && npm run build-script",
    "build-clean": "test -d dist || mkdir -p dist && rm -fr dist/*",
    "build-copy": "cp src/index.html dist/index.html && cp -R src/static/ dist/static/",
    "build-script": "NODE_ENV=production browserify src/javascripts/main.js -o -t sassify -t babelify -t glslify | uglifyjs > dist/main.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uber/deck.gl.git"
  },
  "keywords": [
    "3d",
    "webgl",
    "visualization"
  ],
  "author": "Uber",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uber/deck.gl/issues"
  },
  "homepage": "https://github.com/uber/deck.gl#readme"
}
